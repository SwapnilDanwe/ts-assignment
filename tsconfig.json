{
    "compilerOptions": {
      "target": "es2020",
      "module": "CommonJS",
      "outDir": "dist",
      "rootDir": "./",
      "strict": true,
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "moduleResolution":"node",
      "forceConsistentCasingInFileNames": true, 
      "isolatedModules": true,
      "skipLibCheck": true,
      "preserveConstEnums": true,
      //"noEmit": true,
      "sourceMap": false,
    }
} 